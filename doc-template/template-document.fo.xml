<fo:root xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:svg="http://www.w3.org/2000/svg">
    <fo:layout-master-set>
        <fo:simple-page-master master-name="one">
            <fo:region-body margin-top="40pt" margin-bottom="50pt" margin-left="40pt" margin-right="40pt" />
        </fo:simple-page-master>
    </fo:layout-master-set>
    <fo:page-sequence master-reference="one">
        <fo:flow flow-name="xsl-region-body">
            <fo:block-container height="80%" width="100%" display-align="center">
                <fo:block space-after="30pt" text-align="center">
                    <fo:inline font-weight="bold">@TITLE@</fo:inline>
                </fo:block>
                <fo:block space-after="20pt" text-align="center">
                    <fo:inline>@SUBTITLE@</fo:inline>
                </fo:block>
                <fo:block space-after="20pt" text-align="center">
                    <fo:inline>@DATE@</fo:inline>
                </fo:block>
            </fo:block-container>
            <fo:block page-break-before="always">
            </fo:block>
            @__CONTENT__@
        </fo:flow>
    </fo:page-sequence>
</fo:root>